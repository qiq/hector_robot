<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Config SYSTEM "hector_config.dtd">
<!--
test2: Create WebResources, parse urls, dump them and compose them again.
-->
<Config>
	<Server id="test" lib="RobotServer.la">
		<threads>1</threads>
		<serverHost>*</serverHost>
		<serverPort>1100</serverPort>
		<logConfig>test.log.props</logConfig>
		<ProcessingEngine id="PE_test">
			<Processor id="P_input">
				<threads>1</threads>
				<modules>
					<Module id="M_input" type="perl" lib="WebResourceCreator.pm">
						<param name="urlList">
<![CDATA[						http://qiq:abc@localhost/path?q=a#2
							http://qiq:abc@localhost:8080/path?q=a#2
							http://google.com/path?q=a#2
							http://www.nicnevime.cz:443/
							https://www.vimeovine.cz/ ]]>
						</param>
					</Module>
					<Module id="M_parseurl" lib="UrlParser.la"/>
					<Module id="M_dump" lib="Dump.la"/>
					<Module id="M_composeurl" lib="UrlComposer.la"/>
					<Module id="M_dump" lib="Dump.la"/>
					<Module id="M_output" lib="TestOutput.la"/>
				</modules>
			</Processor>
		</ProcessingEngine>
	</Server>
</Config>
