<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Config SYSTEM "hector_config.dtd">
<!--
test3: Create WebResources, parse urls, resolve DNS and dump result
-->
<Config>
	<Server id="test" lib="RobotServer.la">
		<threads>1</threads>
		<serverHost>*</serverHost>
		<serverPort>1100</serverPort>
		<logConfig>test.log.props</logConfig>
		<ProcessingEngine id="PE_test">
			<Processor id="P_input">
				<threads>1</threads>
				<modules>
					<Module id="M_input" type="perl" lib="WebResourceCreator.pm">
						<param name="urlList">
<![CDATA[						http://www.test.com/foo/bar
							http://www.test.org/
							http://google.com/
							http://www.nicnevime.cz:443/
							https://www.vimeovine.cz/ ]]>
						</param>
					</Module>
					<Module id="M_parseurl" lib="UrlParser.la"/>
					<Module id="M_dump1" lib="Dump.la"/>
					<Module id="M_dns" lib="DnsResolver.pm" type="perl">
						<param name="forwardServer" value="127.0.0.1"/>
						<param name="forwardPort" value="5354"/>
					</Module>
					<Module id="M_dump2" lib="Dump.la"/>
					<Module id="M_output" lib="TestOutput.la"/>
				</modules>
			</Processor>
		</ProcessingEngine>
	</Server>
</Config>
