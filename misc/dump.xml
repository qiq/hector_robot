<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Config SYSTEM "hector_config.dtd">
<!--
Just dumps all resources in file $1
-->
<Config>
	<Server id="dump" lib="RobotServer.la">
		<threads>1</threads>
		<serverHost>*</serverHost>
		<serverPort>1102</serverPort>
		<logConfig>dump.log.props</logConfig>
		<ProcessingEngine id="PE_dump">
			<Processor id="P_dump">
				<threads>1</threads>
				<modules>
					<Module id="M_load" lib="Load.la">
						<param name="filename" value="$1"/>
					</Module>
					<Module id="M_dump" lib="Dump.la"/>
					<Module id="M_output" lib="TestOutput.la">
						<param name="logLevel" value="ERROR"/>
					</Module>
				</modules>
			</Processor>
		</ProcessingEngine>
	</Server>
</Config>
